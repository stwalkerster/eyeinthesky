<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="799081e5-e845-478e-8662-c42e78fe4277" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Eye In The Sky</name>
  <description />
  <settings ref="DotNetProject">
    <options>
      <option name="artifactRules" value="EyeInTheSky/bin/Release/**" />
    </options>
    <parameters>
      <param name="primaryProjectName"><![CDATA[%solutionName%]]></param>
      <param name="solutionName" value="EyeInTheSky" />
      <param name="version" value="2.4" />
    </parameters>
    <build-runners order="RUNNER_6, RUNNER_1, RUNNER_3, RUNNER_7">
      <runner id="RUNNER_6" name="" type="jb.nuget.installer">
        <parameters>
          <param name="nuget.path" value="%teamcity.tool.NuGet.CommandLine.DEFAULT%" />
          <param name="nuget.sources" value="https://nexus.scimonshouse.net/repository/nuget-group/" />
          <param name="nuget.updatePackages.mode" value="sln" />
          <param name="nuget.use.restore" value="restore" />
          <param name="sln.path" value="EyeInTheSky.sln" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="Build" type="MSBuild">
        <parameters>
          <param name="build-file-path" value="EyeInTheSky.sln" />
          <param name="dotNetCoverage.NCover.HTMLReport.File.Sort" value="0" />
          <param name="dotNetCoverage.NCover.HTMLReport.File.Type" value="1" />
          <param name="dotNetCoverage.NCover.Reg" value="selected" />
          <param name="dotNetCoverage.NCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.Reg" value="selected" />
          <param name="dotNetCoverage.NCover3.args" value="//ias .*" />
          <param name="dotNetCoverage.NCover3.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover3.platformVersion" value="NATIVE" />
          <param name="dotNetCoverage.NCover3.reporter.executable.args" value="//or FullCoverageReport:Html:{teamcity.report.path}" />
          <param name="dotNetCoverage.PartCover.Reg" value="selected" />
          <param name="dotNetCoverage.PartCover.executable" value="ddfg" />
          <param name="dotNetCoverage.PartCover.includes" value="[*]*" />
          <param name="dotNetCoverage.PartCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.PartCover.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="msbuild_version" value="mono_4.5" />
          <param name="run-platform" value="x86" />
          <param name="targets" value="Build" />
          <param name="teamcity.step.mode" value="default" />
          <param name="toolsVersion" value="4.0" />
        </parameters>
      </runner>
      <runner id="RUNNER_7" name="Run Tests" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="mono" />
          <param name="command.parameters" value="packages/NUnit.ConsoleRunner.3.9.0/tools/nunit3-console.exe EyeInTheSky.Tests/bin/Release/EyeInTheSky.Tests.dll --teamcity" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Irc_EyeInTheSky_Eits" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <cleanup />
  </settings>
</build-type>

